{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "~5.4 || ^7.0",
        "laravel/framework": "5.4.*",
        "laravel/passport": "^2.0",
        "alexpechkarev/google-maps": "1.0.5",
        "pusher/pusher-php-server": "^2.6",
        "webpatser/laravel-uuid": "2.*",
        "guzzlehttp/guzzle": "^6.2",
        "laravelcollective/html": "5.4.x-dev",
        "laracasts/flash": "^2.0",
        "predis/predis": "^1.1",
        "laravel/dusk": "^1.0",
        "spatie/laravel-uptime-monitor": "^2.0",
        "spatie/laravel-backup": "^4.4",
        "folklore/graphql": "~1.0.0",
        "mpociot/laravel-apidoc-generator": "^2.0",
        "intervention/image": "^2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-debugbar": "^2.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "ln -sr storage/app/public public/storage"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/maddhatter/html.git"
        }
    ]
}
